@using OnlineCasinoModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE" />

    <title>5 Dazzling Hot</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="bgpl"></div>
    <div id="indie"></div>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div id="top-container" class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Online Casino", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Game 2", "Game2", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
                <ul class="nav pull-right">
                    @Html.Partial("~/Views/Member/Login.cshtml",new LoginModel { })
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
            @RenderBody()
    </div>
    <footer>
        <!--p>&copy; @DateTime.Now.Year -  Online Casino <span id="timer">test</span></!--p>-->
    </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")

    @RenderSection("scripts", required: false)

    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Content/js/Utils.js"></script>
    <script src="~/Scripts/extend.js"></script>
    <script src="~/Content/js/framework/workers/computefinals.js"></script>
    <script src="~/Content/js/framework/Constants.js"></script>
    <script src="~/Content/js/framework/util/Timer.js"></script>
    <script src="~/Content/js/framework/util/ReelGenerator.js"></script>
    <script src="~/Content/js/sprites.json.js"></script>
    <script src="~/Content/js/Constants.js"></script>
    <script src="~/Content/js/framework/GameModel.js"></script>
    <script src="~/Content/js/framework/Notification.js"></script>
    <script src="~/Content/js/framework/event/GameEvent.js"></script>
    <script src="~/Content/js/SpinnerUtils.js"></script>
    <script src="~/Content/js/plugins.js"></script>
    <script src="~/Content/js/framework/Finalizer.js"></script>
    <script src="~/Content/js/framework/BaseController.js"></script>
    <script src="~/Content/js/framework/ReelController.js"></script>
    <script src="~/Content/js/framework/SlotEngineController.js"></script>
    <script src="~/Content/js/framework/GameEngineController.js"></script>
    <script src="~/Content/js/framework/ObserverController.js"></script>
    <script src="~/Content/js/framework/MainController.js"></script>

    <script src="~/Content/js/main.js"></script>
</body>
</html>
